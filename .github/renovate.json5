{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "docker-compose": {
    "fileMatch": [
      "^files/.*\\.ya?ml$"
    ]
  },
  "packageRules": [
    {
      "matchManagers": [
        "docker-compose"
      ],
      "enabled": false,
      "matchPackageNames": [
        "/lavalink/"
      ]
    }
  ]
}